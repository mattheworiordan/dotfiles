alias migrate="rake db:migrate db:test:prepare"
alias remigrate="rake db:migrate && rake db:migrate:redo && rake db:schema:dump db:test:prepare"
alias remongrate="rake mongoid:migrate && rake mongoid:migrate:redo"
alias g="git"
alias s="bundle exec rspec"
alias cuc="bundle exec cucumber"
alias gi="gem install"
alias giv="gem install -v"
alias gci="git pull --rebase && rake && git push"
alias tlf="tail -f"
alias b="bundle"
alias be="bundle exec"
alias berk="bundle exec rake"
alias ln='ln -v'
alias mkdir='mkdir -p'
alias ...='../..'
alias l='ls'
alias ll='ls -al'
alias lh='ls -Alh'

alias -g G='| grep'
alias -g M='| less'
alias -g L='| wc -l'
alias -g ONE="| awk '{ print \$1}'"

# matt's settings
alias jam='script/jam'
alias gitx='open -a GitX .'

# Titanium command line
alias titanium="/Library/Application\ Support/Titanium/mobilesdk/osx/1.7.2/titanium.py"
export ANDROID_SDK="/Applications/android-sdk/"

# Titanium logging from Titanium
alias droid_log='adb logcat -s "TiAPI"'
